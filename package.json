{
  "name": "@zzwing/react-image",
  "version": "1.0.1",
  "description": "react image component",
  "main": "dist/index.js",
  "module": "es/index.js",
  "keywords": [
    "react",
    "image",
    "component"
  ],
  "files": [
    "dist",
    "es",
    "src"
  ],
  "scripts": {
    "docz": "cross-env NODE_ENV=development docz dev",
    "docz:build": "docz build",
    "lint": "eslint --ext .js,.jsx --ignore-pattern [vendor] src",
    "build:rollup": "cross-env NODE_ENV=production rollup -c",
    "build:webpack": "cross-env NODE_ENV=production webpack --config=./build/prod.conf.js",
    "dev": "cross-env NODE_ENV=development webpack-serve --config=./build/dev.conf.js --host 0.0.0.0 --port 2222",
    "pub": "npm run lint && npm run build:rollup && npm publish --registry http://registry.npmjs.org --access=public",
    "test": "cross-env NODE_ENV=test jest"
  },
  "author": "zhengzwing@gmail.com",
  "license": "MIT",
  "homepage": "https://zwing.site/react-image/#/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zWingz/react-image.git"
  },
  "bugs": {
    "url": "https://github.com/zWingz/react-image/issues"
  },
  "peerDependencies": {
    "prop-types": ">=15.6.2",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@mdx-js/loader": "^0.15.2",
    "@mdx-js/mdx": "^0.15.2",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "0.11.2",
    "docz-plugin-css": "0.11.0",
    "docz-theme-default": "0.11.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.2",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.6",
    "react-test-renderer": "^16.5.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-plugin-url": "^1.4.0",
    "sass-loader": "^7.0.3",
    "url-loader": "^1.1.1",
    "vue-style-loader": "^4.1.1",
    "jsdom": "^13.0.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3",
    "webpack-serve": "^2.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|scss)$": "<rootDir>/test/utils.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/",
      "<rootDir>/node_modules/",
      "<rootDir>/package.json"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
